---
layout: null
permalink: /assets/posts.json
---

{
  "generatedAt": "{{ site.time | date_to_xmlschema }}",
  "fundamentals": [
    {%- for p in site.fundamentals -%}
    {
      "title": {{ p.title | jsonify }},
      "date": "{{ p.date | date: "%Y-%m-%d" }}",
      "url": "{{ p.url | relative_url }}",
      "collection": "fundamentals"
    }{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ],
  "papers": [
    {%- for p in site.papers -%}
    {
      "title": {{ p.title | jsonify }},
      "date": "{{ p.date | date: "%Y-%m-%d" }}",
      "url": "{{ p.url | relative_url }}",
      "collection": "papers"
    }{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ]
}
